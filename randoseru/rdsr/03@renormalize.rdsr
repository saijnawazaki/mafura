@reference-replace '02@normalize';

body {
  margin: 0;
  font-family: var(--mf-font-sans-serif);
  font-size: var(--mf-font-size);
  font-weight: var(--mf-font-weight);
  line-height: var(--mf-line-height);
  color: var(--mf-color);
  background-color: var(--mf-background-color);
  -webkit-text-size-adjust: 100%;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}